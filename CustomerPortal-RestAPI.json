                                "chargeofsales": {
                                    "type": "string",
                                    "description": "担当営業"
                                },
                                "chargeofenginner": {
                                    "type": "string",
                                    "description": "担当技術"
                                },
                                "customercontractno": {
                                    "type": "string",
                                    "description": "顧客契約番号"
                                },
                                "status": {
                                    "type": "string",
                                    "description": "ステータス"
                                },
                                "amountof": {
                                    "type": "integer",
                                    "description": "契約金額"
                                },
                                "automaticrenewal": {
                                    "type": "boolean",
                                    "description": "自動更新"
                                },
                                "startdate": {
                                    "type": "string",
                                    "description": "契約開始日"
                                },
                                "enddate": {
                                    "type": "string",
                                    "description": "契約終了日"
                                },
                                "enteringdate": {
                                    "type": "string",
                                    "description": "締結日"
                                },
                                "fixeddate": {
                                    "type": "string",
                                    "description": "契約確定日"
                                },
                                "eol": {
                                    "type": "boolean",
                                    "description": "EOL保守の有無"
                                },
                                "typeofconstraction": {
                                    "type": "string",
                                    "description": "構築区分"
                                },
                                "contractid": {
                                    "type": "string",
                                    "description": "契約ID"
                                }
                            }
                        }
                    }
                }
            }
        },
