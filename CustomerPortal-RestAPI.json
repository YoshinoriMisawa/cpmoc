{
    "swagger": "2.0",
    "info": {
        "version": "0.0.3",
        "title": "Customer Portal API"
    },
    "paths": {
        "/customer": {
            "get": {
                "description": "顧客情報を提供",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "顧客のEmailアドレスを検索キーとして設定",
                        "required": true,
                        "type": "string",
                        "format": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "Customer",
                            "type": "object",
                            "required": [
                                "firstname",
                                "email",
                                "companyid"
                            ],
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "description": "顧客ID"
                                },
                                "firstname": {
                                    "type": "string",
                                    "description": "顧客の名前"
                                },
                                "lastname": {
                                    "type": "string",
                                    "description": "顧客の苗字"
                                },
                                "email": {
                                    "type": "string",
                                    "description": "Emailアドレス"
                                },
                                "phone": {
                                    "type": "string",
                                    "description": "電話番号"
                                },
                                "mobile": {
                                    "type": "string",
                                    "description": "携帯電話番号または緊急連絡先"
                                },
                                "fax": {
                                    "type": "string",
                                    "description": "FAX番号"
                                },
                                "companyid": {
                                    "type": "string",
                                    "description": "会社ID（HANAで管理している会社コード（取引先ID、受注先IDなど）を想定）"
                                },
                                "active": {
                                    "type": "boolean",
                                    "description": "この顧客データが有効な場合には、正を設定"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/company": {
            "get": {
                "description": "会社情報を提供",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "会社ID（SAP/HANAで管理している会社コード/取引先ID,受注先IDで使用しているIDを想定）",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "customerid",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "顧客ID"
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "リクエスターのEmailアドレス"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "title": "Company",
                            "type": "object",
                            "required": [
                                "id",
                                "name"
                            ],
                            "properties": {
                                "id": {
                                    "description": "会社ID（SAP/HANAで管理している会社コード/取引先ID,受注先IDで使用しているIDを想定）Inputされたidと同じ値となる",
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string",
                                    "description": "会社名称"
                                },
                                "zip": {
                                    "type": "string",
                                    "description": "郵便番号"
                                },
                                "pref": {
                                    "type": "string",
                                    "description": "都道府県"
                                },
                                "address1": {
                                    "type": "string",
                                    "description": "住所１"
                                },
                                "address2": {
                                    "type": "string",
                                    "description": "住所２"
                                },
                                "addresss3": {
                                    "type": "string",
                                    "description": "住所３"
                                },
                                "phone": {
                                    "type": "string",
                                    "description": "代表電話番号"
                                },
                                "fax": {
                                    "type": "string",
                                    "description": "FAX番号"
                                },
                                "active": {
                                    "type": "boolean",
                                    "description": "このデータが現在有効であれば正を設定"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/companyList": {
            "get": {
                "description": "担当する顧客会社名リストを提供",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "required": true,
                        "type": "string",
                        "description": "リクエスター（NetOne担当者）のEmailアドレス"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "title": "companyList",
                            "type": "object",
                            "required": [
                                "email",
                                "total"
                            ],
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "total": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "Companies": {
                                    "title": "Companies",
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "required": [
                                            "id",
                                            "name"
                                        ],
                                        "properties": {
                                            "id": {
                                                "type": "string",
                                                "description": "会社ID（顧客ID）"
                                            },
                                            "name": {
                                                "type": "string",
                                                "description": "会社名称"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/chargeOfSales": {
            "get": {
                "description": "担当営業情報を提供する",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "required": true,
                        "type": "string",
                        "description": "会社ID（SAP/HANAで管理している会社コード/取引先ID,受注先IDで使用しているIDを想定）"
                    },
                    {
                        "name": "customerid",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "顧客ID（会社IDと同値？）"
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "リクエスターのEmailアドレス"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "title": "SalesList",
                            "type": "object",
                            "required": [
                                "id",
                                "total"
                            ],
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "description": "会社ID"
                                },
                                "total": {
                                    "type": "integer",
                                    "description": "全担当営業数（検索条件に一致した件数）"
                                },
                                "Sales": {
                                    "title": "Sales",
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "required": [
                                            "salesid",
                                            "email"
                                        ],
                                        "properties": {
                                            "salesid": {
                                                "type": "string",
                                                "description": "担当営業ID（社員番号？）"
                                            },
                                            "email": {
                                                "type": "string",
                                                "description": "担当営業のEmailアドレス"
                                            },
                                            "firstname": {
                                                "type": "string",
                                                "description": "担当営業の苗字"
                                            },
                                            "lastname": {
                                                "type": "string",
                                                "description": "担当営業の名前"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/mContractList": {
            "get": {
                "description": "保守契約リスト情報を提供",
