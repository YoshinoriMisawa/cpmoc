                        "type": "string",
                        "description": "個別契約番号"
                    },
                    {
                        "name": "serial",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "シリアル番号"
                    },
                    {
                        "name": "contractor",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "契約先名称"
                    },
                    {
                        "name": "customerContractNo",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "顧客契約番号"
                    },
                    {
                        "name": "enduser",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "エンドユーザー"
                    },
                    {
                        "name": "startdate",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "契約開始日"
                    },
                    {
                        "name": "enddate",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "契約終了日"
                    },
                    {
                        "name": "index",
                        "in": "query",
                        "required": false,
                        "type": "integer",
                        "description": "保守契約一覧を取得する際の開始位置（最初から取得する場合は0を設定）"
                    },
                    {
                        "name": "max",
                        "in": "query",
                        "required": false,
                        "type": "integer",
                        "description": "最大該当数（最大取得数：デフォルト値は50最大値は500）"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "title": "ContractList",
                            "type": "object",
                            "required": [
                                "id",
                                "total",
                                "numof"
                            ],
                            "properties": {
                                "id": {
                                    "description": "会社ID（SAP/HANAで管理している会社コード/取引先ID,受注先ID、納入先コード等で使用しているIDを想定）Inputされたidと同じ値となる",
                                    "type": "string"
                                },
                                "total": {
                                    "type": "integer",
                                    "description": "検索条件に一致した件数"
                                },
                                "index": {
                                    "type": "integer",
                                    "description": "データの開始位置"
                                },
                                "numof": {
                                    "type": "integer",
                                    "description": "データの提供数"
                                },
                                "Contracts": {
                                    "title": "Contracts",
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "required": [
                                            "contract",
                                            "subject"
                                        ],
                                        "properties": {
                                            "contract": {
                                                "type": "string",
                                                "description": "個別契約番号"
                                            },
                                            "startdate": {
                                                "type": "string",
                                                "description": "契約開始日"
                                            },
                                            "enddate": {
                                                "type": "string",
                                                "description": "契約終了日"
                                            },
                                            "subject": {
                                                "type": "string",
                                                "description": "契約名称"
                                            },
                                            "enduser": {
                                                "type": "string",
                                                "description": "エンドユーザー"
                                            },
                                            "chargeofsales": {
                                                "type": "string",
                                                "description": "担当営業"
                                            },
                                            "chargeofenginner": {
                                                "type": "string",
                                                "description": "担当技術"
                                            },
                                            "customercontractno": {
                                                "type": "string",
                                                "description": "顧客契約番号"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
